<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/constraint_layout_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/onboarding6_activity_constraint_layout_constraint_layout_background_color">

	<ImageView
		android:id="@+id/bg_image_view"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginEnd="@dimen/onboarding6_activity_bg_image_view_margin_end"
		android:scaleType="centerCrop"
		android:src="@drawable/bg_2"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintHorizontal_weight="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="0dp"/>

	<Button
		android:id="@+id/get_started_button"
		android:layout_width="@dimen/onboarding6_activity_get_started_button_width"
		android:layout_height="@dimen/onboarding6_activity_get_started_button_height"
		android:layout_marginBottom="@dimen/onboarding6_activity_get_started_button_margin_bottom"
		android:background="?attr/selectableItemBackground"
		android:fontFamily="@font/font_karla_bold"
		android:gravity="center"
		android:text="@string/onboarding6_activity_get_started_button_text"
		android:textColor="#FFFFFF"
		android:textSize="@dimen/onboarding6_activity_get_started_button_text_size"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="24dp"
		tools:layout_editor_absoluteY="628dp"/>

	<ImageView
		android:id="@+id/slide_white_image_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginBottom="@dimen/onboarding6_activity_slide_white_image_view_margin_bottom"
		android:scaleType="center"
		android:src="@drawable/slide_white"
		app:layout_constraintBottom_toTopOf="@+id/get_started_button"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="161dp"
		tools:layout_editor_absoluteY="574dp"/>

	<TextView
		android:id="@+id/plan_ahead_your_expe_text_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="@dimen/onboarding6_activity_plan_ahead_your_expe_text_view_margin_start"
		android:layout_marginTop="@dimen/onboarding6_activity_plan_ahead_your_expe_text_view_margin_top"
		android:fontFamily="@font/font_karla_bold"
		android:gravity="center"
		android:lineSpacingMultiplier="0.92"
		android:text="@string/onboarding6_activity_plan_ahead_your_expe_text_view_text"
		android:textColor="#FFFFFF"
		android:textSize="@dimen/onboarding6_activity_plan_ahead_your_expe_text_view_text_size"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/logo_constraint_layout"
		tools:layout_editor_absoluteX="47dp"
		tools:layout_editor_absoluteY="501dp"/>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/logo_constraint_layout"
		android:layout_width="@dimen/onboarding6_activity_logo_constraint_layout_width"
		android:layout_height="@dimen/onboarding6_activity_logo_constraint_layout_height"
		android:layout_marginTop="@dimen/onboarding6_activity_logo_constraint_layout_margin_top"
		android:background="@color/onboarding6_activity_logo_constraint_layout_background_color"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="156dp"
		tools:layout_editor_absoluteY="411dp">
	
		<TextView
			android:id="@+id/b_text_view"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginEnd="@dimen/onboarding6_activity_b_text_view_margin_end"
			android:layout_marginStart="@dimen/onboarding6_activity_b_text_view_margin_start"
			android:layout_marginTop="@dimen/onboarding6_activity_b_text_view_margin_top"
			android:fontFamily="@font/font_karla_bold"
			android:gravity="center"
			android:text="@string/onboarding6_activity_b_text_view_text"
			android:textColor="#FFFFFF"
			android:textSize="@dimen/onboarding6_activity_b_text_view_text_size"
			app:layout_constraintHorizontal_weight="0.42"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="19dp"
			tools:layout_editor_absoluteY="5dp"/>
	</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>