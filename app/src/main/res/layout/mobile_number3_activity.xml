<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/constraint_layout_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/mobile_number3_activity_constraint_layout_constraint_layout_background_color">

	<Button
		android:id="@+id/continue_button"
		android:layout_width="match_parent"
		android:layout_height="@dimen/mobile_number3_activity_continue_button_height"
		android:layout_marginBottom="@dimen/mobile_number3_activity_continue_button_margin_bottom"
		android:layout_marginStart="@dimen/mobile_number3_activity_continue_button_margin_start"
		android:layout_marginEnd="19dp"
		android:background="@drawable/button_bg"
		android:fontFamily="@font/font_karla_bold"
		android:gravity="center"
		android:text="@string/mobile_number3_activity_continue_button_text"
		android:textColor="#FFFFFF"
		android:textSize="@dimen/mobile_number3_activity_continue_button_text_size"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		tools:layout_editor_absoluteX="19dp"
		tools:layout_editor_absoluteY="565dp"/>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/mobile_number_comple_constraint_layout"
		android:layout_width="@dimen/mobile_number3_activity_mobile_number_comple_constraint_layout_width"
		android:layout_height="@dimen/mobile_number3_activity_mobile_number_comple_constraint_layout_height"
		android:layout_marginStart="@dimen/mobile_number3_activity_mobile_number_comple_constraint_layout_margin_start"
		android:layout_marginTop="@dimen/mobile_number3_activity_mobile_number_comple_constraint_layout_margin_top"
		android:background="@drawable/mobile_number3_activity_mobile_number_comple_constraint_layout_background"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/what_is_your_mobile_text_view"
		tools:layout_editor_absoluteX="19dp"
		tools:layout_editor_absoluteY="184dp">
	
		<ImageView
			android:id="@+id/path_image_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="@dimen/mobile_number3_activity_path_image_view_margin_start"
			android:scaleType="center"
			android:src="@drawable/path"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toRightOf="@+id/united_states_of_ame_constraint_layout"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="46dp"
			tools:layout_editor_absoluteY="20dp"/>
	
		<EditText
			android:id="@+id/edit_text_edit_text"
			android:layout_width="@dimen/mobile_number3_activity_edit_text_edit_text_width"
			android:layout_height="@dimen/mobile_number3_activity_edit_text_edit_text_height"
			android:layout_marginStart="@dimen/mobile_number3_activity_edit_text_edit_text_margin_start"
			android:background="@color/mobile_number3_activity_edit_text_edit_text_background_color"
			android:fontFamily="@font/font_karla_regular"
			android:gravity="start|center_vertical"
			android:hint="@string/mobile_number3_activity_edit_text_edit_text_hint"
			android:inputType="text"
			android:maxLines="1"
			android:textColor="#1C1C1C"
			android:textColorHint="@color/color_hint_text"
			android:textSize="@dimen/mobile_number3_activity_edit_text_edit_text_text_size"
			app:layout_constraintLeft_toRightOf="@+id/path_image_view"
			app:layout_constraintTop_toBottomOf="@+id/mobile_number_text_view"
			tools:layout_editor_absoluteX="72dp"
			tools:layout_editor_absoluteY="21dp"/>
	
		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/united_states_of_ame_constraint_layout"
			android:layout_width="@dimen/mobile_number3_activity_united_states_of_ame_constraint_layout_width"
			android:layout_height="@dimen/mobile_number3_activity_united_states_of_ame_constraint_layout_height"
			android:layout_marginStart="@dimen/mobile_number3_activity_united_states_of_ame_constraint_layout_margin_start"
			android:background="@color/mobile_number3_activity_united_states_of_ame_constraint_layout_background_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="16dp"
			tools:layout_editor_absoluteY="15dp">
		
			<ImageView
				android:id="@+id/group_image_view"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:scaleType="center"
				android:src="@drawable/group_2"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintHorizontal_weight="1"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="2dp"/>
		
			<View
				android:id="@+id/rectangle_constraint_layout"
				android:layout_width="0dp"
				android:layout_height="@dimen/mobile_number3_activity_rectangle_constraint_layout_height"
				android:layout_marginEnd="@dimen/mobile_number3_activity_rectangle_constraint_layout_margin_end"
				android:background="@color/mobile_number3_activity_rectangle_constraint_layout_background_color"
				app:layout_constraintHorizontal_weight="0.5"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="0dp"
				tools:layout_editor_absoluteY="0dp"/>
		
			<ImageView
				android:id="@+id/group_two_image_view"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_marginEnd="@dimen/mobile_number3_activity_group_two_image_view_margin_end"
				android:layout_marginStart="@dimen/mobile_number3_activity_group_two_image_view_margin_start"
				android:layout_marginTop="@dimen/mobile_number3_activity_group_two_image_view_margin_top"
				android:scaleType="center"
				android:src="@drawable/group"
				app:layout_constraintHorizontal_weight="0.42"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				tools:layout_editor_absoluteX="1dp"
				tools:layout_editor_absoluteY="1dp"/>
		</androidx.constraintlayout.widget.ConstraintLayout>
	
		<TextView
			android:id="@+id/mobile_number_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="@dimen/mobile_number3_activity_mobile_number_text_view_margin_start"
			android:layout_marginTop="@dimen/mobile_number3_activity_mobile_number_text_view_margin_top"
			android:fontFamily="@font/font_karla_regular"
			android:gravity="start"
			android:text="@string/mobile_number3_activity_mobile_number_text_view_text"
			android:textColor="#8A8A8A"
			android:textSize="@dimen/mobile_number3_activity_mobile_number_text_view_text_size"
			app:layout_constraintLeft_toRightOf="@+id/path_image_view"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="72dp"
			tools:layout_editor_absoluteY="8dp"/>
	</androidx.constraintlayout.widget.ConstraintLayout>

	<TextView
		android:id="@+id/what_is_your_mobile_text_view"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="@dimen/mobile_number3_activity_what_is_your_mobile_text_view_margin_start"
		android:layout_marginTop="@dimen/mobile_number3_activity_what_is_your_mobile_text_view_margin_top"
		android:fontFamily="@font/font_karla_regular"
		android:gravity="start"
		android:text="@string/mobile_number3_activity_what_is_your_mobile_text_view_text"
		android:textColor="#1C1C1C"
		android:textSize="@dimen/mobile_number3_activity_what_is_your_mobile_text_view_text_size"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/header_constraint_layout"
		tools:layout_editor_absoluteX="19dp"
		tools:layout_editor_absoluteY="150dp"/>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/header_constraint_layout"
		android:layout_width="@dimen/mobile_number3_activity_header_constraint_layout_width"
		android:layout_height="100dp"
		android:layout_marginStart="@dimen/mobile_number3_activity_header_constraint_layout_margin_start"
		android:layout_marginTop="@dimen/mobile_number3_activity_header_constraint_layout_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="19dp"
		tools:layout_editor_absoluteY="48dp">
	
		<TextView
			android:id="@+id/let_sget_started_text_view"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:fontFamily="@font/font_karla_bold"
			android:gravity="start"
			android:text="@string/mobile_number3_activity_let_sget_started_text_view_text"
			android:textColor="#1C1C1C"
			android:textSize="@dimen/mobile_number3_activity_let_sget_started_text_view_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintHorizontal_weight="1"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="32dp"/>
	
		<ImageButton
			android:id="@+id/graphics_back_arrow_button"
			android:layout_width="@dimen/mobile_number3_activity_graphics_back_arrow_button_width"
			android:layout_height="@dimen/mobile_number3_activity_graphics_back_arrow_button_height"
			android:background="?attr/selectableItemBackground"
			android:src="@drawable/graphics_back_arrow"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp"/>
	</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>